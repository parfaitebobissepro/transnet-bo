        <div class="row">
            <div class="col-md-7">
                    <div class="body">
                        <form action="/alerts/addr" class="row clearfix" method="POST">
                            <div class="col-sm-4 mb-0">
                                <div class="form-group">
                                    <div class="form-line m-t-15">
                                        <select name="catName" id="catName">
                                            <option value="all">All</option>
                                            <% categories.forEach((category)=>{ %>
                                                <option value="<%= category._id%>"><%= category.name%></option>
                                            <% }) %>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-4 mb-0">
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" name="address" placeholder="enter une adresse">
                                    </div>
                                </div>
                            </div>
                            
                            <div class="col-sm-4 mb-0 container-button-find">
                                <button class="btn-hover btn-border-radius color-3 mt-2" type="submit">Chercher</button>
                            </div>
                        </form>
                    </div>
                <div class="map" id="map"></div>
            </div>
            <div class="col-md-5">
                <div class="col-sm-12 container-dates">
                    <form class="row">
                        <div class="col-md-4 col-sm-4 col-4">
                            <div class="form-group">
                                <div class="form-line">
                                    <input type="text" name="datedebut" id="datedebut" class="datetimepicker form-control" placeholder="Date début">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-4 col-4">
                            <div class="form-group">
                                <div class="form-line">
                                    <input type="text" name="datefin" id="datefin" class="datetimepicker form-control" placeholder="Date fin">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-4 col-4">
                            <div class="form-group">
                                <div class="form-line">
                                    <button class="btn-hover btn-border-radius color-3 mt-2" type="submit">Chercher</button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="row state-overview" id="map-content-cards">
                    <% alerts.forEach((alert)=>{ %>
                        <div class="js-marker col-lg-6 col-md-6 col-sm-6 col-xs-6" data-lat="<%= alert.lat %>" data-lng="<%= alert.long %>" data-price="<%= alert.category.name %>" data-heure="<%= alert.createdAt.toLocaleTimeString() %>">
                            
                            <div class="info-box5 animate-bar bg-b-purple same-card">
                                <div class="knob-icon">
                                    <!-- <input type="text" class="dial" value="45" data-width="80" data-height="80" data-fgColor="#12"> -->
                                    <img src="<%= alert.category.imgUrl %> " alt="">
                                    <!-- <img src="/uploads/images/zone-danger.png" alt=""> -->
                                </div>
                                <div class="info-box-content">
                                    <span class="info-box-text"><%= alert.category.name%></span>
                                    <div class="progress m-t-20">
                                        <div class="progress-bar l-bg-green shadow-style width-per-100" role="progressbar"
                                            aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
                                    </div>
                                </div>
                                <div class="contain-progress-desc">
                                    <div class="progress-description">
                                        <span>
                                            <i class="material-icons">location_on</i> <%= alert.address %>
                                        </span>
                                        <span>
                                            <i class="material-icons">access_time</i> <%= alert.createdAt.toLocaleTimeString()  %>
                                        </span>
                                        <span>
                                            <i class="material-icons">perm_phone_msg</i> <%= alert.postedBy ? alert.postedBy.phone : 'pas de phone'%>
                                        </span>
                                    </div>
                                </div>
                                <!-- /.info-box-content -->
                            </div>
                        </div>
                    <% }) %> 
                </div>
            </div>
